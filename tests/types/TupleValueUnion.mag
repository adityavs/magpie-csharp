Main (->)
    def a <- Bar ("whoop", 3, true)
    Print a.Bar?.String
    Print a.BarValue
    
    def b <- Frob (123, false)
    Print b.Bar?.String
    Print b.FrobValue
end

Print (a String, b Int, c Bool ->)
    Print a
    Print b.String
    Print c.String
end

Print (b Int, c Bool ->)
    Print b.String
    Print c.String
end

union Foo
    Bar     (String, Int, Bool)
    Frob    (Int, Bool)
end
