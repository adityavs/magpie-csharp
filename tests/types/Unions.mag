Main (->)
    def a <- Bar "whoop"
    
    Print a.Frob?.String
    Print a.BarValue
    
    def b <- Frob
    Print b.Frob?.String
    
    let c <- a.AsBar then Print c
    let c <- b.AsBar then Print "no"
    
end

union Foo
    Bar     String
    Frob
end

AsBar (foo Foo -> Option[String])
    if foo.Bar? then Some foo.BarValue else None[String]
end
