// expected: 0
// expected: 10
// expected: 11
// expected: 12
// expected: 13
// expected: 14

Inc (a Int -> Int) a + 1

Main (->)
    // local variable
    var x <- 0
    Print x.String
    
    x <- 10
    Print x.String

    x <- x + 1
    Print x.String

    x <- + 1
    Print x.String

    x <- x.Inc
    Print x.String

    x <- .Inc
    Print x.String
end
